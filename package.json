{
  "name": "react-native-sensors",
  "version": "3.0.0",
  "description": "A developer friendly approach for sensors in react-native",
  "scripts": {
    "contributor:add": "all-contributors add",
    "contributor:generate": "all-contributors generate",
    "test": "jest",
    "format": "prettier --write $(npx jest --listTests)"
  },
  "main": "index.js",
  "repository": {
    "url": "git@github.com:react-native-sensors/react-native-sensors.git",
    "type": "git"
  },
  "homepage": "https://github.com/react-native-sensors/react-native-sensors",
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Daniel Schmidt",
    "email": "danielmschmidt92@gmail.com",
    "url": "http://danielmschmidt.de"
  },
  "jest": {
    "timers": "fake",
    "projects": [
      {
        "displayName": "Unit"
      },
      {
        "displayName": "Prettier",
        "runner": "jest-runner-prettier",
        "moduleFileExtensions": [
          "js",
          "jsx",
          "json",
          "ts",
          "tsx",
          "css",
          "less",
          "scss",
          "graphql",
          "md",
          "markdown"
        ],
        "testMatch": [
          "**/*.js",
          "**/*.jsx",
          "**/*.json",
          "**/*.ts",
          "**/*.tsx",
          "**/*.css",
          "**/*.less",
          "**/*.scss",
          "**/*.graphql",
          "**/*.md",
          "**/*.markdown"
        ],
        "testPathIgnorePatterns": [
          "package.json",
          "package-lock.json"
        ]
      }
    ]
  },
  "babel": {
    "presets": [
      "react-native"
    ]
  },
  "license": "MIT",
  "peerDependencies": {
    "react-native": ">=0.39.0",
    "prop-types": "^15.6.0"
  },
  "dependencies": {
    "jest-runner-prettier": "^0.2.6",
    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "all-contributors-cli": "4.11.1",
    "babel-core": "6.26.3",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.3",
    "prettier": "1.12.1",
    "react": "16.3.2",
    "react-native": "0.56.1",
    "regenerator-runtime": "0.11.1"
  }
}
